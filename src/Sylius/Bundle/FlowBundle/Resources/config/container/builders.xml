<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
                               http://symfony.com/schema/dic/services/services-1.0.xsd"
>

    <parameters>
        <parameter key="sylius_flow.builder.class">Sylius\Bundle\InstallerBundle\Setup\Builder\SetupBuilder</parameter>
    </parameters>

    <services>
        <service id="sylius_flow.builder" class="%sylius_flow.builder.class%">
            <argument type="service" id="service_container" />
            <argument type="service" id="sylius_flow.storage" />
        </service>
    </services>

</container>
